{
    "name": "jigius/twig-ext-widget",
    "description": "Twig Extension Widget",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jigius",
            "email": "jigius@gmail.com"
        }
    ],
    "require": {
        "twig/twig": ">=2",
